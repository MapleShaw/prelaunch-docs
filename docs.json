{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "willow",
  "name": "PreLaunch Documentation",
  "description": "Official documentation for PreLaunch - Next.js landing page template for startup product validation",
  "url": "https://docs.prelaunch.online",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#006AD4",
    "light": "#007FFF",
    "dark": "#006AD4"
  },
  "seo": {
    "indexHiddenPages": false,
    "includeInSitemap": true
  },
  "metadata": {
    "og:site_name": "PreLaunch Documentation",
    "og:type": "website",
    "twitter:card": "summary_large_image",
    "twitter:site": "@msjiaozhu"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Core Features",
            "pages": [
              "features/waitlist",
              "features/voting",
              "features/roadmap",
              "features/blog",
              "features/multilanguage",
              "features/payments"
            ]
          },
          {
            "group": "Customization",
            "pages": [
              "customization/configuration",
              "customization/themes",
              "customization/content"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Get PreLaunch",
          "href": "https://www.prelaunch.online",
          "icon": "book-open-cover"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/MapleShaw/prelaunch",
          "icon": "github"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@prelaunch.online"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://github.com/MapleShaw/prelaunch"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/msjiaozhu"
    }
  }
}